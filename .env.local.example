# Liveblocks Real-time Collaboration (EASY TO DISABLE)
# Set to 'false' to completely disable real-time features
NEXT_PUBLIC_ENABLE_REALTIME=true
NEXT_PUBLIC_ENABLE_LIVEBLOCKS=true

# Liveblocks API Keys (get free at liveblocks.io)
# Leave blank to disable Liveblocks
NEXT_PUBLIC_LIVEBLOCKS_PUBLIC_KEY=
LIVEBLOCKS_SECRET_KEY=

# Existing Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Auth
NEXTAUTH_SECRET=your_nextauth_secret